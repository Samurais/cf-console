%tr.oddeven{:id => "app-" + @name + "-envvar-" + app_envvar_item[:var_name]}
  %td= app_envvar_item[:var_name]
  %td.editableText= app_envvar_item[:var_value]
  %td.tdcenter= link_to image_tag("delete.png", :alt => "Unset the variable"),
                app_unset_var_url(@name, :var_name => app_envvar_item[:var_name]),
                :id => "unset-var-link", :remote => true,
                :method => :put, :confirm => "Are you sure?"
